<section>
  <h2 class="border-b border-b-gray-500 text-5xl mb-8">Projects</h2>
  <dl>
    {% for project in resume.projects %}
      <dt class="mb-2">
        <a class="text-xl" href="{{ project.url }}">{{ project.name }}</a>
      </dt>
      <dd class="mb-4">{{ project.description }}</dd>
    {% endfor %}
  </dl>
</section>