<section>
  <h2 class="border-b border-b-gray-500 text-5xl mb-8">Skills</h2>
  <dl>
    {% for skill in resume.skills %}
      <dt class="mb-2 text-xl">
        {{ skill.name }}
      </dt>
      <dd class="mb-4">
        {{ skill.keywords | join(', ') }}
      </dd>
    {% endfor %}
  </dl>
</section>
