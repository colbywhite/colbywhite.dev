{
  "name": "colbywhite.dev",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:postCache": "cross-env NODE_ENV=production ts-node scripts/buildPosts.ts",
    "resume": "cross-env NODE_ENV=development ts-node scripts/buildResume.ts",
    "build:remix": "remix build",
    "dev": "run-p dev:*",
    "dev:remix": "remix watch",
    "dev:postCache": "cross-env NODE_ENV=development ts-node scripts/buildPosts.ts",
    "dev:wrangler": "cross-env NODE_ENV=development wrangler pages dev ./public",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "start": "remix-serve build",
    "test": "vitest",
    "typecheck": "tsc -b",
    "validate": "run-p \"test -- --run\" lint typecheck"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build"
  ],
  "dependencies": {
    "@remix-run/cloudflare": "^1.15.0",
    "@remix-run/cloudflare-pages": "^1.15.0",
    "@remix-run/react": "^1.15.0",
